<% if @stores.any? %>
  <ul class="no-bullet-list no-list-indent">
    <% @stores.each do |store| %>
      <li>
      <%= link_to list_path(store.id) do %>
      <nav class="expand-row tan-bg center-text padding black-font large-font margin-top">
        <%= store.name %>
      </nav>
      <% end %>
      </li>
    <% end %>
  </ul>
<% end %>
